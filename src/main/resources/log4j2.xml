<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="GraphQLServer">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d [%-40.40t] %-5 %-30.30c{1} - %n%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>