type Query {
    countries: [Country]
}

type Country {
    code: ID
    name: String
    population: String
    area: String
    capital: City
    province: [Province]
}

type City {
    name: String
    population: Int
    geoLocation: GeoCoord
    elevation: Int
    province: String
}

type GeoCoord {
    latitude: Float
    Logitude: Float
}

type Province {
    name: String
    populatin: Int
    capital: City
}